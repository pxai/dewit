{
    "name": "Dewit!",
    "description": "Just dewit!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab","alarms", "notifications"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/dewit16.png",
        "32": "/images/dewit32.png",
        "48": "/images/dewit48.png",
        "128": "/images/dewit128.png"
      }
    },
    "icons": {
      "16": "/images/dewit16.png",
      "32": "/images/dewit32.png",
      "48": "/images/dewit48.png",
      "128": "/images/dewit128.png"
    },
    "options_page": "options.html",
    "web_accessible_resources": [{
      "resources": ["sounds/gold.mp3", "sounds/cock.mp3", "sounds/shot.mp3", "sounds/load.mp3"],
      "matches": []
    }]
  }